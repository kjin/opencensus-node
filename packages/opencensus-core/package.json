  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.00"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/continuation-local-storage": "^3.2.1",
    "@types/debug": "0.0.30",
    "@types/end-of-stream": "^1.4.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/semver": "^5.5.0",
    "@types/shimmer": "^1.0.1",
    "@types/uuid": "^3.4.3",
    "gts": "^0.5.4",
    "ncp": "^2.0.0",
    "google-cloud": "^0.57.0",
    "googleapis": "27.0.0",
    "mocha": "^5.0.4",
    "ts-node": "^4.0.0",
    "typescript": "~2.6.1"
  },
  "dependencies": {
    "async_hooks": "^1.0.0",
    "continuation-local-storage": "^3.2.1",
    "debug": "^3.1.0",
    "end-of-stream": "^1.4.1",
    "require-in-the-middle": "^2.2.1",
    "semver": "^5.5.0",
    "shimmer": "^1.2.0",
    "uuid": "^3.2.1"
  }
